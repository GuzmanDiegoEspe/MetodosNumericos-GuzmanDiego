    "import numpy as np \n",
    "from math import sqrt\n",
    "A = np.array([[5,2,4,3],[2,4,6,1],[-1,4,1,0],[3,-2,0,4]])\n",
    "m = A.shape[0]\n",
    "n = A.shape[1]\n",
    "suma_raiz = 0\n",
    "for i in range (m):\n",
    "    for j in range (n):\n",
    "        suma_raiz += pow(A[i][j],2)\n",
    "    Respuesta = sqrt(suma_raiz)\n",
    "print(round(Respuesta,4))"
   ]
  }
