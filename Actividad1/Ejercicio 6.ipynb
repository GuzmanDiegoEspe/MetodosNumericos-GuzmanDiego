    "import numpy as pn \n",
    "\n",
    "x = pn.pi/3\n",
    "n = 51 \n",
    "polinomio = 0\n",
    "\n",
    "for k in range (n):\n",
    "    polinomio = polinomio + (-1)**k*x**(2*k+1) / pn.math.factorial(2*k+1)\n",
    "    print(k,polinomio)"
   ]
